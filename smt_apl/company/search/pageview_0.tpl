  e.preventDefault();
  var $tab = $('.tab03 a');
  var index = $tab.index(this);
  $tab.removeClass('current');
  $(this).addClass('current');
  $('[class^=table-layout0]')
   .removeClass('block-on').addClass('block-off')
   .eq(index).addClass('block-on').removeClass('block-off');
 });
 $('.tab03 a').eq(0).trigger('click');
});
</script>
